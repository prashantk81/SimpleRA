LOAD Student
LOAD Advisor
RES1 <- CROSS Student Advisor
RES2 <- SELECT Advisor_id == Adv_Id FROM RES1
RES3 <- SELECT Adv_age > 40 FROM RES2
RES4 <- PROJECT Stud_Id, Advisor_id FROM RES3
RENAME Advisor_id TO adv_id FROM RES4
RENAME Stud_Id TO stud_id FROM RES4
PRINT RES4
CLEAR RES4
CLEAR RES3
CLEAR RES2
CLEAR RES1
CLEAR Advisor
CLEAR Student