LOAD Advisor
LOAD Student
LOAD Marks
RES1 <- SELECT Adv_Id == 1 FROM Advisor
RES2 <- SELECT Maths_marks > 70 FROM Marks
RES3 <- CROSS RES1 RES2
RES4 <- SELECT Advisor_id == 1 FROM Student
RES5 <- CROSS RES3 RES4
RES6 <- SELECT Stud_sex == 1 FROM RES5
RENAME RES3_Stud_Id TO stud_id FROM RES6
PRINT RES6
CLEAR RES5
CLEAR RES4
CLEAR RES3
CLEAR RES2
CLEAR RES1
CLEAR Marks
CLEAR Student
CLEAR Advisor